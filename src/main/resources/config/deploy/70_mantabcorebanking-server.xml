<?xml version="1.0" encoding="UTF-8"?>
<srv name="mantabcorebanking" class="org.jpos.q2.iso.QServer" logger="Q2">
    <attr name="port" type="java.lang.Integer">10001</attr>
    <attr name="minSessions" type="java.lang.Integer">1</attr>
    <attr name="maxSessions" type="java.lang.Integer">100</attr>
    <channel class="org.jpos.iso.channel.WBKChannel" packager="org.jpos.iso.packager.GenericPackager" logger="Q2">
       <property name="packager-config" value="config/packager/iso87mantabcorebanking.xml" />
       <!-- timeout before session is dropped - disabled, used default -->
       <!--
       <property name="timeout" value="30000" />
       -->
	    <filter class='com.mii.komi.jpos.filter.IncomingOutgoingDebugFilter' direction='both' />
    </channel>
    <!-- whitelist - disabled
    <property name="allow" value="127.0.0.1"/>
    -->
    <in>mantabcorebanking-in</in>
    <out>mantabcorebanking-out</out>
</srv>

